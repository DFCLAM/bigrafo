     -->

    


    <!-- http://purl.org/spar/fabio/ArchivalDocument -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/ArchivalDocument">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Expression"/>
        <rdfs:comment xml:lang="en">An archival document is a realization of the content related to an archival record. It can be exemplified as a book, a document, a letter, a database, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">archival document</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/ArchivalRecord -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/ArchivalRecord">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Work"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realization"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/ArchivalDocument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An archival record connotes a material created or received by a person, family, or organization, public or private, in the conduct of their affairs that is preserved because of the enduring value contained in the information it contains or as evidence of the function and the responsibilities of its creator.</rdfs:comment>
        <rdfs:label xml:lang="en">archival record</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/ArtisticWork -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/ArtisticWork">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Work"/>
        <rdfs:comment xml:lang="en">It describes any work regarded as art in its widest sense, including works from literature and music, visual art, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">artistic work</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Expression -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Expression">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Expression"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#embodiment"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Manifestation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#realizationOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Work"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Expression can only have part or be part of another fabio:Expression.  Moreover, it can be a representation only of a fabio:Work, and  it can be embodied only in fabio:Manifestation(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR expression, restricted to expressions of fabio:Works.  For your latest research paper, the preprint submitted to the publisher, and the final published version to which the publisher assigned a unique digital object identifier, are both expressions of the same work.  </rdfs:comment>
        <rdfs:label xml:lang="en">expression</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Item -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Item">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Item"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#exemplarOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Manifestation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Item can only have part or be part of another fabio:Item. Moreover, it can be an exemplar only of a fabio:Manifestation.</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR item, restricted to exemplars of fabio:Manifestations.  An example of a fabio:Item is a printed copy of a journal article on your desk, or a PDF file of that article that you purchased from a publisher and that now resides in digital form on your computer hard drive.  </rdfs:comment>
        <rdfs:label xml:lang="en">item</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Manifestation -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Manifestation">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Manifestation"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#embodimentOf"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Expression"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#exemplar"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/spar/fabio/Item"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description xml:lang="en">A fabio:Manifestation can only have part or be part of another fabio:Manifestation.  Moreover, it can be an embodiment only of a fabio:Expression and it can be exemplified only by fabio:Item(s).</dc:description>
        <rdfs:comment xml:lang="en">A subclass of FRBR manifestation, restricted to manifestations of fabio:Expressions. fabio:Manifestation specifically applies to electronic (digital) as well as to physical manifestations of expressions.  

Examples of different manifestations of a single &apos;version of record&apos; expression of a scholarly work include an article in a print journal or the on-line version of that article as a web page.</rdfs:comment>
        <rdfs:label xml:lang="en">manifestation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/ScholarlyWork -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/ScholarlyWork">
        <rdfs:subClassOf rdf:resource="http://purl.org/spar/fabio/Work"/>
        <rdfs:comment xml:lang="en">A work that reports scholarly activity on a particular topic, either published in written form, or delivered orally at a meeting.</rdfs:comment>
        <rdfs:label xml:lang="en">scholarly work</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/spar/fabio/Work -->

    <owl:Class rdf:about="http://purl.org/spar/fabio/Work">
        <rdfs:subClassOf rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#part"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
                        <owl:allValuesFrom rdf:resource="http://purl.org/vocab/frbr/core#Work"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
